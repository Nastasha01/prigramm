a=[1, 2, 3, 4, 5]
b=a 
print(a,b)
b[2]=100
print(a,b)

 //////////////////////////////////
 
a=[1, 2, 3, 4, 5]
b=a 
print(a,b)
b[2]=100
print(a,b)
b=a.copy()
b[2]=100
print(a,b)

